{

  publisher <
    name = American Society for Microbiology
  >

  {
    plugin = org.lockss.plugin.pub2web.asm.ASMscienceJournalsPlugin
    param[base_url] = http://www.asmscience.org/
    implicit < status ; year ; name ; param[volume_name] >
    comment[platform] = Pub2Web
  
    {
      title <
        name = EcoSal Plus ;
        eissn = 2324-6200
      >
      
      comment[structure] = this seems to be organized as a reference, not as a journal.
      
      param[journal_id] = ecosalplus
      
      au < exists ; 2004-2005 ; EcoSal Plus Volume 1 ; 1 >
      au < exists ; 2006-2007 ; EcoSal Plus Volume 2 ; 2 >
      au < exists ; 2008-2009 ; EcoSal Plus Volume 3 ; 3 >
      au < exists ; 2010-2011 ; EcoSal Plus Volume 4 ; 4 >
      au < exists ; 2012-2013 ; EcoSal Plus Volume 5 ; 5 >
      au < exists ; 2014-2015 ; EcoSal Plus Volume 6 ; 6 >
      au < expected ; 2015-2016 ; EcoSal Plus Volume 7 ; 7 >

    }
  
    {
      title <
        name = Microbiology Spectrum ;
        eissn = 2165-0497
      >
      
      param[journal_id] = microbiolspec
      
      au < exists ; 2013 ; Microbiology Spectrum 2013 ; 1 >
      au < exists ; 2014 ; Microbiology Spectrum 2014 ; 2 >
      au < exists ; 2015 ; Microbiology Spectrum 2015 ; 3 >
      au < expected ; 2016 ; Microbiology Spectrum 2016 ; 4 >

    }

  }

}
