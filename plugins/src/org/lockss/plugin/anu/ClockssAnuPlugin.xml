<map>
  <entry>
    <string>plugin_status</string>
    <string>testing-EJ</string>
  </entry>
  <entry>
    <string>plugin_identifier</string>
    <string>org.lockss.plugin.anu.ClockssAnuPlugin</string>
  </entry>
  <entry>
    <string>plugin_version</string>
    <string>1</string>
  </entry>
  <entry>
    <string>plugin_feature_version_map</string>
    <map>
      <entry>
        <string>Metadata</string>
        <string>1</string>
      </entry>
      <entry>
        <string>Poll</string>
        <string>1</string>
      </entry>
      <entry>
        <string>Substance</string>
        <string>1</string>
      </entry>
    </map>
  </entry>
  <entry>
    <string>plugin_name</string>
    <string>Australian National University Press Plugin (CLOCKSS)</string>
  </entry>
  <entry>
    <string>au_name</string>
    <string>"Australian National University Press Plugin (CLOCKSS), Base URL %s, JID %s, Volume %s", base_url, journal_id, volume_name</string>
  </entry>
  <entry>
    <string>au_start_url</string>
    <string>"%spublications/%s", base_url, journal_id</string>
  </entry>
  <entry>
    <string>plugin_per_host_permission_path</string>
    <string>/lockss.txt</string>
  </entry>
  <entry>
    <!-- the au_permitted_host_pattern must also find match in lockss-daemon config
         org.lockss.crawler.allowedPluginPermittedHosts -->
    <string>au_permitted_host_pattern</string>
    <list>
      <string>(fonts|ajax)\.googleapis\.com</string>
      <string>press(-files)?\.anu\.edu\.au</string> 
<!--       <string>(press(-files)?|style)\.anu\.edu\.au</string> -->
    </list>
  </entry>
  <entry>
    <string>au_additional_url_stems</string>
    <list>
      <string>"%s", to_http(base_url)</string>
      <string>"%s", to_https(base_url)</string>
      <string>http://press-files.anu.edu.au/</string>
      <string>https://press-files.anu.edu.au/</string>
      <string>http://style.anu.edu.au/</string>
      <string>https://style.anu.edu.au/</string>
    </list>
  </entry>
  <entry>
    <string>au_def_new_content_crawl</string>
    <long>1209600000</long>
  </entry>
  <entry>
    <string>au_def_pause_time</string>
    <long>3000</long>
  </entry>
  <entry>
    <!-- start page to landing page, is difficult to determine 
        as the first good link may be on the 3rd page of listings,
        however, daemon may recompute depth so only need to add 1 to depth -->
    <string>au_refetch_depth</string>
    <int>3</int>
  </entry>
  <entry>
    <string>au_crawlrules</string>
    <list>
      <string>1,"^https?://((fonts|ajax)\.googleapis\.com)/.*\.(bmp|css|eot|gif|ico|jpe?g|js|otf|png|svg|tif?f|ttf|woff.?)(\?.*)?"</string>
      <string>1,"^https?://([^.]+\.anu\.edu\.au)/.*\.(bmp|css|eot|gif|ico|jpe?g|js|otf|png|svg|tif?f|ttf|woff.?)$"</string>
      <!-- in case there is a bad link, seems that redirects to search page, don't follow these links -->
      <string>2,"\?search="</string>
      <string>1, "^https?://[^/]+/lockss.txt$"</string>
      <string>1,"^https?://%s%spublications/(journals/)?%s(\?page=[0-9]+)?$", url_host(base_url), url_path(base_url), journal_id</string>
      <string>1,"^https?://%s%s((\?p=|node/)[0-9]+|.*?(issue|volume|no-[1-9]|winter)-%s(-[12n].+?)?)(/download)?$", url_host(base_url), url_path(base_url), volume_name</string>
<!--  ^https?://[^/]+\/(?:pub.+?)(?:(\?p=|node/)[0-9]+|-(issue|volume|no-[1-9]|winter)-([0-9]{1,4}|[ivxlcdm]+)(-[12n].+?)?)(\/download)?$ -->
      <string>1,"^https?://press-files\.anu\.edu\.au/downloads/press/[^/]+/(pdf|html)/", base_url</string>
    </list>
  </entry>
  <entry>
    <string>au_substance_url_pattern</string>
    <list>
      <string>"^https?://press-files.anu.edu.au/downloads/press/[^/]+/pdf/book\.pdf"</string>
    </list>
  </entry>
  <entry>
    <string>plugin_config_props</string>
    <list>
      <org.lockss.daemon.ConfigParamDescr>
        <key>base_url</key>
        <displayName>Base URL</displayName>
        <description>Usually of the form http://&lt;journal-name&gt;.com/</description>
        <type>3</type>
        <size>40</size>
        <definitional>true</definitional>
        <defaultOnly>false</defaultOnly>
      </org.lockss.daemon.ConfigParamDescr>
      <org.lockss.daemon.ConfigParamDescr>
        <key>journal_id</key>
        <displayName>Journal Id</displayName>
        <description>Identifier for journal (often used as part of file names)</description>
        <type>1</type>
        <size>100</size>
        <definitional>true</definitional>
        <defaultOnly>false</defaultOnly>
      </org.lockss.daemon.ConfigParamDescr>
      <org.lockss.daemon.ConfigParamDescr>
        <key>volume_name</key>
        <displayName>Volume</displayName>
        <type>1</type>
        <size>40</size>
        <definitional>true</definitional>
        <defaultOnly>false</defaultOnly>
      </org.lockss.daemon.ConfigParamDescr>
    </list>
  </entry>
  <entry>
    <string>au_url_normalizer</string>
    <string>org.lockss.plugin.anu.AnuUrlNormalizer</string>
  </entry>
  <entry>
    <string>text/html_link_extractor_factory</string>
    <string>org.lockss.plugin.anu.AnuHtmlLinkExtractorFactory</string>
  </entry>
  <entry>
    <string>text/html_crawl_filter_factory</string>
    <string>org.lockss.plugin.anu.AnuHtmlCrawlFilterFactory</string>
  </entry>
  <entry>
    <string>text/html_filter_factory</string>
    <string>org.lockss.plugin.anu.AnuHtmlHashFilterFactory</string>
  </entry>
