<map>
  <entry>
    <string>plugin_status</string>
    <string>development</string>
  </entry>
  <entry>
    <string>plugin_identifier</string>
    <string>org.lockss.plugin.silverchair.oup.ScOUPPlugin</string>
  </entry>
  <entry>
    <string>plugin_version</string>
    <string>1</string>
  </entry>
  <entry>
    <string>plugin_feature_version_map</string>
    <map>
      <entry>
        <string>Poll</string>
        <string>1</string>
      </entry>
      <entry>
        <string>Substance</string>
        <string>1</string>
      </entry>
      <entry>
        <string>Metadata</string>
        <string>1</string>
      </entry>
    </map>
  </entry>
  <entry>
    <string>plugin_name</string>
    <string>Silverchair Oxford University Press Plugin</string>
  </entry>
  <entry>
    <string>plugin_config_props</string>
    <list>
      <org.lockss.daemon.ConfigParamDescr>
        <key>base_url</key>
        <displayName>Base URL</displayName>
        <description>Usually of the form http://&lt;journal-name&gt;.com/</description>
        <type>3</type>
        <size>40</size>
        <definitional>true</definitional>
        <defaultOnly>false</defaultOnly>
      </org.lockss.daemon.ConfigParamDescr>
      <org.lockss.daemon.ConfigParamDescr>
        <key>journal_id</key>
        <displayName>Journal Identifier</displayName>
        <description>Identifier for journal</description>
        <type>1</type>
        <size>40</size>
        <definitional>true</definitional>
        <defaultOnly>false</defaultOnly>
      </org.lockss.daemon.ConfigParamDescr>
      <org.lockss.daemon.ConfigParamDescr>
        <key>year</key>
        <displayName>Year</displayName>
        <description>Four digit year (e.g., 2004)</description>
        <type>4</type>
        <size>4</size>
        <definitional>true</definitional>
        <defaultOnly>false</defaultOnly>
      </org.lockss.daemon.ConfigParamDescr>
    </list>
  </entry>
  <entry>
    <string>au_name</string>
    <string>"Silverchair Oxford University Press Plugin, Base URL %s, Journal ID %s, Year %d", base_url, journal_id, year</string>
  </entry>
  <entry>
    <string>au_start_url</string>
    <string>"%s%s/list-of-issues/%d", base_url, journal_id, year</string>
  </entry>
  <entry>
    <string>au_crawlrules</string>
    <list>
      <string>1,"^https?://([^.]+\.(googleapis)\.com)/"</string>
      <!-- I used the same old list of cdn -->
      <string>1,"^https?://(%s|[^.]+\.(silverchair\.netdna-cdn|silverchair-?cdn)\.com/).*\.(bmp|css|eot|gif|ico|jpe?g|js|otf|png|svg|tif?f|ttf|woff)(\?.*)?$", url_host(base_url)</string>
      <string>1,"^https?://%s/%s/(article(-pdf)?|issue|)/[0-9]+/", url_host(base_url), journal_id</string>
    </list>
  </entry>
  <entry>
    <string>au_substance_url_pattern</string>
    <list>
      <string>"^%s\%s\article\", base_url, journal_id</string>
    </list>
  </entry>
  <entry>
    <string>au_crawl_depth</string>
    <int>2</int>
  </entry>
  <entry>
    <string>au_def_new_content_crawl</string>
    <long>1209600000</long>
  </entry>
  <entry>
    <string>au_def_pause_time</string>
    <long>3000</long>
  </entry>
  <entry>
    <string>plugin_fetch_rate_limiter_source</string>
    <string>key:Silverchair</string>
  </entry>
  <entry>
    <string>text/html_link_extractor_factory</string>
    <string>org.lockss.plugin.silverchair.ScHtmlLinkExtractorFactory</string>
  </entry>
