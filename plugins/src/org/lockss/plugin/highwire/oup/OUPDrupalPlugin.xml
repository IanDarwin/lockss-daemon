    <string>plugin_parent</string>
    <string>org.lockss.plugin.highwire.HighWireDrupalPlugin</string>
  </entry>
  <entry>
    <string>plugin_parent_version</string>
    <string>59</string>
  </entry>
  <entry>
    <string>plugin_feature_version_map</string>
    <map>
      <entry>
        <string>Substance</string>
        <string>2</string>
